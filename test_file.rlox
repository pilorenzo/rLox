fun fib(n) {
    if (n <= 1) return n;
    var prev2 = n - 2;
    var prev1 = n - 1;
    return fib(prev2) + fib(prev1);
}


for (var i = 0; i < 15; i = i + 1) {
    print fib(i);
}


